{
  "kind": "collectionType",
  "collectionName": "tours",
  "info": {
    "singularName": "tour",
    "pluralName": "tours",
    "displayName": "tour"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "isFeatured": {
      "type": "boolean"
    },
    "rating": {
      "type": "decimal"
    },
    "destinations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::destination.destination",
      "mappedBy": "tours"
    },
    "imageURL": {
      "type": "string"
    }
  }
}
